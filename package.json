{
  "name": "fp-ts-learn",
  "version": "0.10.31",
  "private": true,
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "./node_modules/.bin/nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/src/main",
    "start:dev": "PRETTY_PRINT=true nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "start:mocked": "NODE_ENV=test MOCKED=true PRETTY_PRINT=true nest start --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:modules": "PRETTY_PRINT=true NODE_ENV=test ./node_modules/.bin/mocha -r ts-node/register --config ./test/modules/.mocharc.yml",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "PRETTY_PRINT=true NODE_ENV=test ./node_modules/.bin/mocha -r ts-node/register --config ./test/e2e/.mocharc.yml",
    "gendoc": "protoc --doc_out=./docs/rpc --doc_opt=markdown,docs/rcp/asyncapi.md docs/service.proto"
  },
  "dependencies": {
    "@nestjs-fuib/amqp-server": "^0.2.0",
    "@nestjs-fuib/vault": "^0.3.1",
    "@nestjs/common": "^8.0.6",
    "@nestjs/core": "^8.0.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/microservices": "^8.0.6",
    "@nestjs/platform-fastify": "^8.0.6",
    "@nestjs/sequelize": "^8.0.0",
    "@nestjs/swagger": "^5.0.9",
    "@types/moment": "^2.13.0",
    "amqplib": "^0.8.0",
    "argon2": "^0.28.2",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.13.2",
    "dayjs": "^1.10.4",
    "debug": "^2.6.8",
    "env-var": "^6.0.4",
    "fp-ts": "^2.9.4",
    "fp-ts-contrib": "^0.1.21",
    "got": "^11.8.3",
    "i18n-iso-countries": "^6.5.0",
    "io-ts": "^2.2.13",
    "io-ts-types": "^0.5.13",
    "ioredis": "^3.2.1",
    "jaeger-client": "^3.17.2",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^1.25.0",
    "moment": "^2.24.0",
    "monocle-ts": "^2.3.5",
    "nestjs-pino": "^1.1.3",
    "newtype-ts": "^0.3.4",
    "nock": "^13.0.4",
    "opentracing": "^0.14.4",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "pino-pretty": "^4.1.0",
    "pp-errors": "^3.0.19",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.3.0",
    "sequelize": "^6.11.0",
    "sequelize-typescript": "^2.1.1",
    "source-map-support": "^0.5.16",
    "uuid": "^3.4.0",
    "uuid-validate": "0.0.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/amqp-connection-manager": "^2.0.8",
    "@types/amqplib": "^0.5.13",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/luxon": "^2.0.7",
    "@types/mocha": "^8.0.3",
    "@types/nock": "^11.1.0",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^7.0.0",
    "@types/uuid-validate": "0.0.1",
    "bugsbunny": "^6.2.2",
    "mocha": "^8.1.1",
    "prettier": "^2.0.4",
    "should": "^13.2.3",
    "supertest": "^4.0.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
